{
	"name": "kiwi-t9",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"react": "^16.8.3",
		"react-dom": "^16.8.3",
		"react-scripts": "2.1.5"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"format": "prettier --write \"{src,public}/**/*.{js,json,css,html}\"",
		"commit": "git-cz"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@commitlint/cli": "^7.5.2",
		"@commitlint/config-conventional": "^7.5.0",
		"cz-conventional-changelog": "^2.1.0",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.5",
		"prettier": "1.16.4"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,json,css,md}": [
			"prettier --write",
			"git add"
		]
	}
}
